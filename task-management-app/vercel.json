{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@angular-devkit/build-angular:application"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "outputDirectory": "dist/task-management-app"
}
